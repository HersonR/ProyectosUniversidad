    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.CodigoCatedratico)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NombresCatedratico)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ApellidosCatedratico)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EstadoCatedratico)
            </td>
