@model IEnumerable<ExamenFinal01.Models.Catedratico>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.CodigoCatedratico)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NombresCatedratico)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ApellidosCatedratico)
